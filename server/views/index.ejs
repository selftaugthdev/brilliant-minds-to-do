<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Ideas</title>
</head>
<body>
    <div id="ideasContainer">
        <% ideas.forEach(function(idea) { %>
            <div class="idea">
                <h2><%= idea.title %></h2>
                <p><%= idea.description %></p>
            </div>
        <% }); %>
    </div>
</body>
</html>
